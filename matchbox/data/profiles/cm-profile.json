{
  "id": "cm-profile",
  "name": "Raspberry Pi CM Profile",
  "boot": {
    "kernel": "/assets/alpine/vmlinuz-rpi",
    "initrd": ["/assets/alpine/initramfs-rpi"],
    "args": [
      "console=serial0,115200",
      "console=tty1",
      "ip=dhcp",
      "modloop=/assets/alpine/modloop-rpi",
      "alpine_repo=http://dl-cdn.alpinelinux.org/alpine/v3.21/main",
      "pkgs=curl,wget,ca-certificates"
    ],
    "cloud_id": "orchestrator"
  }
}
