{
  "id": "cm-profile",
  "name": "Raspberry Pi CM Profile",
  "boot": {
    "kernel": "/assets/alpine/vmlinuz-lts",
    "initrd": ["/assets/alpine/initramfs-lts"],
    "args": [
      "console=serial0,115200",
      "console=tty1",
      "ip=192.168.11.129::192.168.11.1:255.255.255.0:bladee:eth0:off",
      "alpine_repo=http://dl-cdn.alpinelinux.org/alpine/v3.21/main",
      "modloop=/assets/alpine/modloop-lts"
    ]
  }
}
